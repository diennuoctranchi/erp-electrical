<% content_for :title do %><%= t('.create') %><% end %>
<% content_for :page_title do %>
    <a href="<%= erp_electrical.backend_blogs_path %>"><i class="fa fa-th"></i> <%= t('.blogs') %></a> / 
    <%= t('.create') %><% end %>
<% content_for :page_toolbar do %><div class="btn-group"><div class="clearfix"></div></div><% end %>
<div class="row">
    <div class="col-md-12">
        <% if @blog.errors.any? %>
            <%= erp_form_control("form_error_message") %>
        <% end %>
        <%= render 'form', blog: @blog %>
    </div>
</div>